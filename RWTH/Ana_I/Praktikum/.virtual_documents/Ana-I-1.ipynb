function fib(limit) 
    
    fib_1 = 1
    fib_2 = 1
    current = 0
    
    while current <= limit
        
        current = fib_1 + fib_2
        println(current)
        fib_1 = fib_2
        fib_2 = current 
    end
end

fib(10000)








function A(n)
    return sum( ((-1)^(i + 1)) * (i^2) for i=1:((2*n) - 1))
end

